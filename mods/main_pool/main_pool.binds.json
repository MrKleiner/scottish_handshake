{
	"click": [
		{
			"selector": "mpool flist flist-entry.league",
			"function": "$this.list_league_matches",
			"pass_event": false,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "mpool flist flist-entry.match",
			"function": "$this.list_match_struct",
			"pass_event": false,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "mpool flist flist-entry.struct_entry",
			"function": "$this.list_media",
			"pass_event": false,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "mpool flist flist-entry.media_entry",
			"function": "$this.load_fullres_media",
			"pass_event": false,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "img#pic_fullres_preview",
			"function": "$this.viewing_fullres = false;$('img#pic_fullres_preview').remove",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#exec_dlq_download",
			"function": "$all.ft.process_download_queue",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		}
	],

	"keydown": [
		{
			"selector": "body",
			"function": "$this.img_cycle_lr",
			"pass_event": true,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "body",
			"function": "$this.select_all_in_folder",
			"pass_event": true,
			"pass_element": false,
			"pass_params": ""
		}
	],
	"contextmenu": [
		{
			"selector": "flist-entry.media_entry",
			"function": "$this.add_media_entry_to_selection",
			"pass_event": true,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "img#pic_fullres_preview",
			"function": "$this.download_image_from_fullres",
			"pass_event": true,
			"pass_element": true,
			"pass_params": ""
		}
	]
}