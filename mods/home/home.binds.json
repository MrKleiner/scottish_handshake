{
	"click": [
		{
			"selector": "#app_topbar #nav_login",
			"function": "$all.login.load_module",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#app_topbar #nav_sign_out",
			"function": "$all.login.logout",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#app_topbar #nav_enter_edit",
			"function": "$all.admin.load_module",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#app_topbar #nav_go_home",
			"function": "$all.main_pool.home_button",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "#app_topbar #nav_display_help, #help_overlay",
			"function": "$all.core.display_help",
			"pass_event": false,
			"pass_element": false,
			"pass_params": ""
		},
		{
			"selector": "lzdropdown .lz_menu_entries [dropdown_set]",
			"function": "lzdrops.set_active",
			"pass_event": false,
			"pass_element": true,
			"pass_params": ""
		},
		{
			"selector": "[haslizdropdown], lzdropdown",
			"function": "lzdrops.showhide",
			"pass_event": false,
			"pass_element": true,
			"pass_params": "",
			"else": "lzdrops.showhide"
		}
	]
}